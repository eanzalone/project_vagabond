<div class="form">
<h1>Login</h1>

<%= form_tag sessions_path do %>
	Email:<br>
	<%= text_field :email, placeholder: "Email", :class => 'form-control' %><br>
	Password:<br>
	<%= password_field :password, placeholder: "Password", :class => 'form-control' %><br>
	<%= submit_tag "Login" %>
<% end %>
</div>