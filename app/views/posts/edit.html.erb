<%= form_for @post do |f| %>
	<div>
	<p>Title:</p>
		<%= f.text_field :title %>
	</div>
    <br>
    <div>
    <p>Tags:</p>
        <%= f.select :tag_list, [['Food'],['News'],['Music'],['Bars'],['Nightlife']] %>
    </div>
    <div>
    <p>Body:</p>
    	<%= f.text_field :body %>
    </div>
    <br>

    <%= f.submit "update", class:"btn btn-primary"%>
<% end %>