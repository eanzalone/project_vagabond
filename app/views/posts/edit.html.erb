<%= form_for @post do |f| %>
<div>
	<p>Title:</p>
  <%= f.text_field :title %>
</div>
<br>
<div>
    <p>Body:</p>
    <%= f.text_field :body %>
</div>
<br>
<div>
    <%= f.select :location, [['Bernal Heights'],['Castro'],['Chinatown'],['Dogpatch'],['Embarcadero'],['Excelsior'],['Financial District'],['Glen Park'],['Haight'],['Hayes Valley'],['Marina'],['Mission'],['Mission Bay'],['Nob Hill'],['Noe Valley'],['North Beach'],['Pacific Heights'],['Potrero Hill'],['Richmond'],['Russian Hill'],['Tenderloin']] %>
</div>

<%= f.submit "update", class:"btn btn-primary"%>
<% end %>